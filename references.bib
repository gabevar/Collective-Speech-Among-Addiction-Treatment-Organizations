}

@article{atuahene2018,
  title = {"{{OUR TAXES ARE TOO DAMN HIGH}}": {{INSTITUTIONAL RACISM}}, {{PROPERTY TAX ASSESSMENTS}}, {{AND THE FAIR HOUSING ACT}}},
  author = {Atuahene, Bernadette},
  date = {2018},
  journaltitle = {Northwestern University Law Review},
  volume = {112},
